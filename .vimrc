syntax enable
set number

packadd! dracula
colorscheme dracula

set ts=4
set sts=4
set shiftwidth=4
set smartindent
set nowrap
set incsearch

"set noerrorbells
"set expandtab
"set smartcase

"set noswapfile
"set nobackup
"set undodir=~/.vim/undodir
"set undofile

set colorcolumn=80
highlight ColorColumn ctermbg=0 guibg=lightgrey
